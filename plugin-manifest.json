    {
      "label": "Playlist",
      "value": "playlist",
      "documentation": {
        "link": "https://github.com/applicaster/zapp-pipes-provider-brightcove#readme",
        "input_description": "Playlist Id",
        "input_placeholder": "Please enter Playlist Id",
        "input_description_image_url": ""
