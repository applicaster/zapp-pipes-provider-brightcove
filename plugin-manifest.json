    {
      "label": "Folder",
      "value": "folder",
      "documentation": {
        "link": "https://github.com/applicaster/zapp-pipes-provider-brightcove#readme",
        "input_description": "Folder Id",
        "input_placeholder": "Please enter Folder Id",
        "input_description_image_url": ""
      }
    },
    {
      "label": "Search",
      "value": "search",
      "documentation": {
        "link": "https://github.com/applicaster/zapp-pipes-provider-brightcove#readme",
        "input_description": "Search Qieru",
        "input_placeholder": "Please enter Search Query",
        "input_description_image_url": ""
      }
    }
  ],
  "custom_configuration_fields": [
    {
      "type": "text",
      "key": "account_id",
      "tooltip_text": "Account ID in Brightcove Videocloud. For more information, see: https://support.brightcove.com/managing-account-settings#account"
    },
    {
      "type": "text",
      "key": "client_id",
      "tooltip_text": "client ID that is used in getting access_token for the Brightcove API. For more information, see: https://support.brightcove.com/managing-api-authentication-credentials#Creating_client_registrations"
    },
    {
      "type": "text",
      "key": "client_secret",
      "tooltip_text": "client secret that is used in getting access_token for the Brightcove API. For more information, see: https://support.brightcove.com/managing-api-authentication-credentials#Creating_client_registrations"
    }
  ]
